{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/static/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/favicon.ico": {
      "Cache-Control": "public, max-age=86400"
    },
    "/manifest.json": {
      "Cache-Control": "public, max-age=86400"
    },
    "/logo.png": {
      "Cache-Control": "public, max-age=86400"
    }
  },
  "clean_urls": true,
  "https_only": true,
  "error_page": "index.html"
} 